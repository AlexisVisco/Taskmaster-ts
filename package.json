{
  "name": "babel-typescript-sample",
  "version": "0.7.0",
  "license": "MIT",
  "scripts": {
    "run-dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts --p=9898 --cfg=cfg.json",
    "type-check": "tsc --noEmit",
    "build": "tsc --emitDeclarationOnly && babel src --out-dir dist --extensions \".ts,.tsx\""
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.42",
    "@babel/core": "^7.0.0-beta.42",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.42",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "@babel/preset-typescript": "^7.0.0-beta.42",
    "@types/node": "^9.6.1",
    "typescript": "^2.7.2"
  }
}
